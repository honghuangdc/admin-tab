{
  "name": "soybean-admin-tab",
  "version": "1.1.7",
  "description": "a tab component of admin template, support vue2 and vue3",
  "private": false,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc",
    "build": "npm run typecheck && vite build",
    "build:vercel": "cross-env VITE_IS_VERCEL=1 vite build",
    "release": "standard-version",
    "publish": "npm publish --access public",
    "preview": "vite preview",
    "lint": "eslint --fix ./ --ext .vue,.js,jsx,.ts,tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honghuangdc/soybean-admin-tab.git"
  },
  "keywords": [
    "admin",
    "tab",
    "admin-tab",
    "vue2",
    "vue3"
  ],
  "author": "Soybean (honghuangdc@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/honghuangdc/soybean-admin-tab/issues"
  },
  "homepage": "https://github.com/honghuangdc/soybean-admin-tab#readme",
  "dependencies": {
    "colord": "^2.9.2",
    "css-render": "^0.15.8",
    "vue-demi": "^0.12.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.6",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "vite": "2.8.6",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-windicss": "^1.8.2",
    "vue": "^3.2.31",
    "vue-tsc": "^0.32.1",
    "windicss": "^3.5.1"
  }
}
